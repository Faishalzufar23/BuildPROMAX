{
  "expo": {
    "name": "BuildPROMAX",
    "slug": "BuildPROMAX",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/promax.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/promax.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/promax.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zufar23.BuildPROMAX"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "my-scheme",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ea101ffe-7d21-474c-ab13-855a80e40e54"
      }
    }
  }
}
